{
  "branches": [
    "main",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    "@semantic-release/git",
    ["@semantic-release/exec", {
      "prepareCmd": "npm run build",
      "publishCmd": "npx @capgo/cli@latest bundle upload --channel ${nextRelease.channel} --version ${nextRelease.version}"
    }]
  ]
}